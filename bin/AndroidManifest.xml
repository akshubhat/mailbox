<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techblogon.loginexample"
    android:versionCode="3"
    android:versionName="3.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.techblogon.loginexample.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name=".HomeActivity1"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.techblogon.loginexample.HomeActivity.logout" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name=".SignUPActivity"/>
         
           <activity android:name=".profile"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.techblogon.loginexample.HomeActivity.profile" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
          
          <activity android:name=".compose"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.techblogon.loginexample.HomeActivity.compose" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
               
                  
         </application>
    
    
    

</manifest>